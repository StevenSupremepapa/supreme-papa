public class Album extends Media
{
    public String Composer;
    public boolean winGoldenGlobeAward;

    public Album()
    {
        super();
        Composer = "Ennio Morricone";
        winGoldenGlobeAward = true;
    }
    
    public Album(double price,String name,int time,boolean isPopular,String Composer,boolean winGoldenGlobeAward)
    {
        super(price,name,time,isPopular);
        this.Composer = Composer;
        this.winGoldenGlobeAward = winGoldenGlobeAward;
    }
    
    public String getMostPopularCommentForHisOrHerMusic()
    {
        return "The most popular comment for is or her music is: ";
    }
    
    
    public String toString()
    {
        return super.toString() + 
        "Composer: " + Composer + 
        "winGoldenGlobeAward: " + winGoldenGlobeAward + "\n";
    }
}
